CXXFLAGS=-MMD -g -I /usr/include/YaST2
LDFLAGS = -ly2storage

PROG=TestDisk TestDiskLog TestProbing TestLvm TestMd TestLoop

all: $(PROG)

TestDisk: TestDisk.o

TestDiskLog: TestDiskLog.o

TestProbing: TestProbing.o

TestLvm: TestLvm.o

TestMd: TestMd.o

TestLoop: TestLoop.o

clean:
	rm -f *.d *.o $(PROG)

ifneq "$(strip $(wildcard *.d))" ""
  include $(wildcard *.d)
endif
