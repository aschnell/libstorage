
CXXFLAGS = -MMD -g -I /usr/include/YaST2

LDFLAGS = -ly2storage

PROGRAMS = TestDisk TestDiskLog TestProbing TestLvm TestMd TestLoop		\
	 SaveGraph

all: $(PROGRAMS)

TestDisk: TestDisk.o

TestDiskLog: TestDiskLog.o

TestProbing: TestProbing.o

TestLvm: TestLvm.o

TestMd: TestMd.o

TestLoop: TestLoop.o

SaveGraph: SaveGraph.o

clean:
	rm -f *.d *.o $(PROGRAMS)

ifneq "$(strip $(wildcard *.d))" ""
  include $(wildcard *.d)
endif
